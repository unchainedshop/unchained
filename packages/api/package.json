{
  "name": "@unchainedshop/api",
  "description": "GraphQL API layer for the Unchained Engine with Express/Fastify adapters and MCP server",
  "version": "5.0.0-alpha.0",
  "main": "lib/api-index.js",
  "types": "lib/api-index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/api-index.js",
      "types": "./lib/api-index.d.ts"
    },
    "./fastify": {
      "import": "./lib/fastify/index.js",
      "types": "./lib/fastify/index.d.ts"
    },
    "./express": {
      "import": "./lib/express/index.js",
      "types": "./lib/express/index.d.ts"
    },
    "./lib/*": {
      "import": "./lib/*",
      "types": "./lib/*"
    },
    "./lib/*.js": {
      "import": "./lib/*.js",
      "types": "./lib/*.d.ts"
    },
    "./index.html": {
      "import": "./index.html"
    }
  },
  "scripts": {
    "clean": "tsc -b --clean",
    "build": "tsc -b",
    "prepublishOnly": "npm run clean && npm run build",
    "watch": "tsc -w",
    "test": "node --test",
    "test:watch": "node --test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unchainedshop/unchained.git"
  },
  "keywords": [
    "unchained",
    "ecommerce",
    "api"
  ],
  "authors": [
    "Pascal Kaufmann",
    "Mikael Araya",
    "Vedran Rudelj",
    "JoÃ«l Meiller"
  ],
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/unchainedshop/unchained/issues"
  },
  "homepage": "https://github.com/unchainedshop/unchained#readme",
  "peerDependencies": {
    "@ai-sdk/mcp": "^0.0.*",
    "@fastify/cookie": ">= 11 < 12",
    "@fastify/multipart": ">= 9 < 10",
    "@fastify/static": ">= 8 < 9",
    "@modelcontextprotocol/sdk": ">= 1 < 2",
    "ai": ">= 5 < 6",
    "cookie-parser": ">= 1.4 < 2",
    "express": ">= 5 < 6",
    "fastify": ">= 5.2 < 6",
    "graphql-yoga": ">= 5.9 < 6",
    "multer": ">= 2 < 3"
  },
  "peerDependenciesMeta": {
    "graphql-yoga": {
      "optional": false
    },
    "@modelcontextprotocol/sdk": {
      "optional": true
    },
    "ai": {
      "optional": true
    },
    "@ai-sdk/mcp": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "@fastify/cookie": {
      "optional": true
    },
    "@fastify/multipart": {
      "optional": true
    },
    "@fastify/static": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "cookie-parser": {
      "optional": true
    },
    "multer": {
      "optional": true
    }
  },
  "dependencies": {
    "@unchainedshop/core": "^5.0.0-alpha.0",
    "@unchainedshop/events": "^5.0.0-alpha.0",
    "@unchainedshop/logger": "^5.0.0-alpha.0",
    "@unchainedshop/roles": "^5.0.0-alpha.0",
    "@unchainedshop/utils": "^5.0.0-alpha.0",
    "dataloader": "^2.2.3",
    "expiry-map": "^2.0.0",
    "graphql-scalars": "^1.24.2",
    "jsonwebtoken": "^9.0.2",
    "mime": ">= 4 < 5",
    "p-memoize": "^8.0.0",
    "zod": "^3.25.76 || ^4"
  },
  "devDependencies": {
    "@ai-sdk/mcp": "^0.0.*",
    "@fastify/cookie": "^11.0.2",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^25.0.0",
    "ai": "^5.0.80",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "fastify": "^5.4.0",
    "multer": "^2.0.1",
    "typescript": "^5.8.3"
  }
}
