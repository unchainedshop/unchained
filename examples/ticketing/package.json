  },
  "engineStrict": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "lint": "prettier -w .",
    "clean": "tsc -b --clean",
    "build": "tsc -b",
    "start": "node --env-file .env.defaults --env-file-if-exists=.env lib/boot.js",
    "dev": "node --no-warnings --env-file .env.defaults --env-file-if-exists=.env --loader ts-node/esm --watch boot.ts",
    "test:run:integration": "run-p -r test:integration:*",
    "test:integration:start": "node --no-warnings --env-file .env.defaults --env-file ../../.env.tests --env-file-if-exists=../../.env --import ./load_env.js --loader ts-node/esm --watch boot.ts",
    "test:integration:run": "node --no-warnings --env-file ../../.env.tests --env-file-if-exists=.env --test-global-setup=../../tests/helpers.js --test --test-concurrency=1 tests/*.test.*"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/multipart": "^9.0.3",
    "@fastify/session": "^11.1.0",
    "@unchainedshop/platform": "^4.1.0",
    "@unchainedshop/plugins": "^4.1.0",
    "@unchainedshop/ticketing": "^4.1.0",
    "fastify": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
