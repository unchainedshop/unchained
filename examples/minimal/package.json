{
  "name": "@unchainedshop/example-minimal",
  "description": "Minimal Bootstrap Project for Unchained Engine",
  "version": "0.29.0",
  "license": "EUPL-1.2",
  "homepage": "https://unchained.shop",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unchainedshop/unchained.git"
  },
  "contributors": [
    "Vedran Rudelj <vedran@unchained.shop> (https://unchained.shop)",
    "Pascal Kaufmann <pascal@unchained.shop> (https://unchained.shop)",
    "Marco Wettstein <maw@panter.ch>",
    "Simon Emanuel Schmid <simon@schmid.io>"
  ],
  "bugs": {
    "url": "https://github.com/unchainedshop/unchained/issues"
  },
  "meteor": {
    "mainModule": {
      "client": false,
      "server": "boot.js"
    }
  },
  "scripts": {
    "lint": "eslint . --cache  --fix --ext=jsx --ext=js",
    "link:cp": "meteor npm link ../controlpanel && npm link ../controlpanel",
    "unlink:cp": "meteor npm unlink @unchainedshop/controlpanel && npm unlink @unchainedshop/controlpanel",
    "dev": "cross-env METEOR_PACKAGE_DIRS=../../packages UI_ENDPOINT=http://localhost:4000 meteor --no-release-check --no-lint -p 4010",
    "build": "cross-env METEOR_PACKAGE_DIRS=../../packages UI_ENDPOINT=http://localhost:4000 meteor build --server-only --directory .build",
    "debug": "cross-env METEOR_PACKAGE_DIRS=../../packages UI_ENDPOINT=http://localhost:4000 meteor debug -p 4010"
  },
  "dependencies": {
    "@unchainedshop/controlpanel": "latest",
    "@babel/runtime": "^7.5.4",
    "apollo-server-express": "^2.6.9",
    "bcrypt": "^3.0.6",
    "braintree": "^2.18.0",
    "dotenv": "^8.0.0",
    "emoji-flags": "^1.2.0",
    "faker": "^4.1.0",
    "graphql": "^14.4.2",
    "hashids": "^1.2.2",
    "i18n-iso-countries": "^4.2.0",
    "iconv-lite": "^0.5.0",
    "isomorphic-unfetch": "^3.0.0",
    "locale": "^0.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isnumber": "^3.0.3",
    "mjml": "^4.3.1",
    "moment": "^2.24.0",
    "moniker": "^0.1.2",
    "mustache": "^3.0.1",
    "node-cache": "^4.2.0",
    "node-sheets": "^1.0.0",
    "open": "^6.4.0",
    "simpl-schema": "^1.5.5",
    "stripe": "^7.4.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  }
}
